<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- services -->

	<bean id="serviceClient" class="Bootstrap.ServiceClient">
		<property name="helloService" ref="helloServiceRpcProxy"></property>
	</bean>
	
	<!-- 服务的代理类 ServiceProxy -->
	<bean id="helloServiceRpcProxy" class="">
		<property name="interfaceName"></property>
		<property name="version"></property>
		<property name="group"></property>
	</bean>
	




</beans>